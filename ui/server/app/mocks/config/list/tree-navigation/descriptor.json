{
    "name": "crafter.studio-ui.plugins.tree-navigation",
    "version": "0.1.0",
    "base_url": "sdo/tree-navigation/",
    "main": "tree-navigation.js",

    "config": {
        "sections": [{
            "label": "Assets",
            "content": {
                "type": "asset",
                "serviceProvider": "RemoteStudioServices",
                "service": "Asset.list"
            }
        }, {
            "label": "Descriptors",
            "content": {
                "type": "descriptor",
                "serviceProvider": "RemoteStudioServices",
                "service": "Descriptor.list"
            }
        }, {
            "label": "Templates",
            "content": {
                "type": "template",
                "serviceProvider": "RemoteStudioServices",
                "service": "Template.list"
            }
        }]
    }
}
